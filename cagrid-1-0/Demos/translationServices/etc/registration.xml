  <ServiceGroupEPR>
    <wsa:Address>INDEX_SERVICE_URL</wsa:Address>
  </ServiceGroupEPR>
  <!-- Specifies that the registration will be renewed every 10 minutes -->
  <RefreshIntervalSecs>600</RefreshIntervalSecs>
  <Content xsi:type="agg:AggregatorContent" xmlns:agg="http://mds.globus.org/aggregator/types">
    <agg:AggregatorConfig xsi:type="agg:AggregatorConfig">
      <agg:GetMultipleResourcePropertiesPollType
			xmlns:ns8="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BioMaterial"
			xmlns:ns11="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BQS"
			xmlns:ns19="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery"
			xmlns:ns14="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.Description"
			xmlns:ns18="gme://GenePattern.caBIG/1/gridExtensions"
			xmlns:ns17="gme://cagrid.geworkbench.columbia.edu/1/edu.columbia.geworkbench.cagrid.microarray"
			xmlns:ns6="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.AuditAndSecurity"
			xmlns:ns21="http://annualdemo.cagrid.nci.nih.gov/MageTranslationServices/types"
			xmlns:ns2="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.ArrayDesign"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:ns15="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.Measurement"
			xmlns:ns22="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"
			xmlns:ns16="gme://cagrid.geworkbench.columbia.edu/1/edu.columbia.geworkbench.cagrid.cluster.som"
			xmlns:ns0="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BioSequence"
			xmlns:ns13="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BioAssayData"
			xmlns:ns7="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain"
			xmlns:ns20="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLResultSet"
			xmlns:ns1="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.QuantitationType"
			xmlns:ns5="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.HigherLevelAnalysis"
			xmlns:ns12="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.Protocol"
			xmlns:ns3="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BioEvent"
			xmlns:ns4="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.Experiment"
			xmlns:ns9="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.DesignElement"
			xmlns:ns10="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BioAssay">
        <!-- Specifies that the index should refresh information
            every 300000 milliseconds (once every 5 minutes) -->
        <agg:PollIntervalMillis>300000</agg:PollIntervalMillis>
        <!-- specifies all Resource Properties that should be retrieved from the service -->
      </agg:GetMultipleResourcePropertiesPollType>
    </agg:AggregatorConfig>
    <agg:AggregatorData/>
  </Content>
</ServiceGroupRegistrationParameters>
