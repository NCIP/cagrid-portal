	<info organisation="SampleApplications" module="webssoclientexample" revision="${project.version}">
		<license name="caGrid" url="http://www.cagrid.org/wiki/CaGrid:LICENSE-1.2" />
		<description>WebSSO Client Example</description>
	</info>

	<configurations>
		<!-- Public Configurations-->
		<conf name="default" visibility="public" />
		<conf name="war" visibility="public" extends="default" />
