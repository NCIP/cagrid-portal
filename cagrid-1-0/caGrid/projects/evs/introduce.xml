 <ns3:Services xsi:type="ns3:ServicesType" xmlns:ns3="gme://gov.nih.nci.cagrid.introduce/1/Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns3:Service name="EVSGridService" namespace="http://cagrid.nci.nih.gov/EVSGridService" packageName="gov.nih.nci.cagrid.evsgridservice" resourceFrameworkType="main" xsi:type="ns3:ServiceType">
   <ns4:Methods xsi:type="ns4:MethodsType" xmlns:ns4="gme://gov.nih.nci.cagrid.introduce/1/Methods">
    <ns4:Method description="Returns the service's security metadata." isImported="true" isProvided="true" name="getServiceSecurityMetadata" xsi:type="ns4:MethodType">
     <ns4:Output qName="ns5:ServiceSecurityMetadata" isArray="false" isClientHandle="false" xmlns:ns5="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"/>
     <ns4:ImportInformation namespace="http://security.introduce.cagrid.nci.nih.gov/ServiceSecurity" packageName="gov.nih.nci.cagrid.introduce.security.stubs" portTypeName="ServiceSecurityPortType" wsdlFile="ServiceSecurity.wsdl"/>
     <ns4:ProviderInformation providerClass="gov.nih.nci.cagrid.introduce.security.service.globus.ServiceSecurityProviderImpl"/>
    </ns4:Method>
    <ns4:Method description="Returns all the vocabularies present in the Description Logic in caCORE 3.1 EVS service." isImported="false" isProvided="false" name="getVocabularyNames" xsi:type="ns4:MethodType">
     <ns4:Inputs/>
     <ns4:Output qName="ns6:DescLogicConceptVocabularyName" description="This class will contain the input parameters used by caCORE EVS API to retrieve Description Logic vocabulary names" isArray="true" xmlns:ns6="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.evs.service"/>
     <ns4:Exceptions/>
    </ns4:Method>
    <ns4:Method description="Returns all Metathesaurus Sources contained in the EVS." isImported="false" isProvided="false" name="getMetaSources" xsi:type="ns4:MethodType">
     <ns4:Inputs/>
     <ns4:Output qName="ns7:Source" description="The source is a knowledge base." isArray="true" xmlns:ns7="gme://caCORE.caCORE/3.1/gov.nih.nci.evs.domain"/>
     <ns4:Exceptions/>
    </ns4:Method>
    <ns4:Method description="Searches a valid Vocabulary such as NCI Thesaurus and returns Description Logic concepts that meet the search criteria." isImported="false" isProvided="false" name="searchDescLogicConcept" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns8:EVSDescLogicConceptSearchParams" description="This class will contain the input parameters used by caCORE EVS API to retrieve Description Logic Concept data" isArray="false" name="eVSDescLogicConceptSearchParams" xmlns:ns8="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.evs.service"/>
     </ns4:Inputs>
     <ns4:Output qName="ns9:DescLogicConcept" description="The DescLogicConcept class represents the fundamental vocabulary entity in the NCI Thesaurus. " isArray="true" xmlns:ns9="gme://caCORE.caCORE/3.1/gov.nih.nci.evs.domain"/>
     <ns4:Exceptions>
      <ns4:Exception description="Thrown if invalid inputs are used, such as null or empty concepts or sources." name="InvalidInputException" qname="ns10:InvalidInputException" xmlns:ns10="http://cagrid.nci.nih.gov/EVSGridService/types"/>
     </ns4:Exceptions>
    </ns4:Method>
    <ns4:Method description="Searches NCI meta thesaurus and returns Meta Thesaurus information that meet the search criteria." isImported="false" isProvided="false" name="searchMetaThesaurus" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns11:EVSMetaThesaurusSearchParams" description="This class will contain the input parameters used by caCORE EVS API to retrieve Meta Thesaurus Concepts" isArray="false" name="eVSMetaThesaurusSearchParams" xmlns:ns11="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.evs.service"/>
     </ns4:Inputs>
     <ns4:Output qName="ns12:MetaThesaurusConcept" description="MetaThesaurusConcept is the fundamental vocabulary entity in the NCI MetaThesaurus." isArray="true" xmlns:ns12="gme://caCORE.caCORE/3.1/gov.nih.nci.evs.domain"/>
     <ns4:Exceptions>
      <ns4:Exception description="Thrown if invalid inputs are used, such as null or empty concepts or sources." name="InvalidInputException" qname="ns13:InvalidInputException" xmlns:ns13="http://cagrid.nci.nih.gov/EVSGridService/types"/>
     </ns4:Exceptions>
    </ns4:Method>
    <ns4:Method description="Searches a valid vocabulary in NCI thesaurus for history information." isImported="false" isProvided="false" name="getHistoryRecords" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns14:EVSHistoryRecordsSearchParams" description="This class will contain the input parameters used by caCORE EVS API to retrieve History data" isArray="false" name="eVSHistoryRecordsSearchParams" xmlns:ns14="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.evs.service"/>
     </ns4:Inputs>
     <ns4:Output qName="ns15:HistoryRecord" description="The HistoryRecord holds history information for the specified concept" isArray="true" xmlns:ns15="gme://caCORE.caCORE/3.1/gov.nih.nci.evs.domain"/>
     <ns4:Exceptions>
      <ns4:Exception description="Thrown if invalid inputs are used, such as null or empty concepts or sources." name="InvalidInputException" qname="ns16:InvalidInputException" xmlns:ns16="http://cagrid.nci.nih.gov/EVSGridService/types"/>
     </ns4:Exceptions>
    </ns4:Method>
    <ns4:Method description="Searches the Meta Thesaurus based on Source code." isImported="false" isProvided="false" name="searchSourceByCode" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns17:EVSSourceSearchParams" description="This class will contain the input parameters used by caCORE EVS API to retrieve Meta Thesaurus Concepts." isArray="false" name="eVSSourceSearchParams" xmlns:ns17="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.evs.service"/>
     </ns4:Inputs>
     <ns4:Output qName="ns18:MetaThesaurusConcept" description="MetaThesaurusConcept is the fundamental vocabulary entity in the NCI MetaThesaurus." isArray="true" xmlns:ns18="gme://caCORE.caCORE/3.1/gov.nih.nci.evs.domain"/>
     <ns4:Exceptions>
      <ns4:Exception description="Thrown if invalid inputs are used, such as null or empty concepts or sources." name="InvalidInputException" qname="ns19:InvalidInputException" xmlns:ns19="http://cagrid.nci.nih.gov/EVSGridService/types"/>
     </ns4:Exceptions>
    </ns4:Method>
   </ns4:Methods>
   <ns20:ResourcePropertiesList xsi:type="ns20:ResourcePropertiesListType" xmlns:ns20="gme://gov.nih.nci.cagrid.introduce/1/Resources">
    <ns20:ResourceProperty qName="ns21:ServiceMetadata" description="caGrid standard service metadata" fileLocation="serviceMetadata.xml" populateFromFile="true" register="true" xsi:type="ns20:ResourcePropertyType" xmlns:ns21="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"/>
   </ns20:ResourcePropertiesList>
   <ns3:description xsi:type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">The main EVS grid service.</ns3:description>
  </ns3:Service>
 </ns3:Services>
