<ns1:ExtensionDescription extensionType="INTRODUCE_GDE" version="1.2" xmlns:ns1="gme://gov.nih.nci.cagrid.introduce/1/Extension">
	<ns1:IntroduceGDEExtensionDescriptionType displayName="Gaards UI" name="cagrid_gaards_ui">
		<menus xmlns="http://www.cagrid.org/1/grape">
		<menu>
           <title>Security</title>
            <description>tools</description>
            <showIcons>false</showIcons>
            <submenus>
           
        <menu>
            <title>My Account</title>
            <description>tools</description>
            <showIcons>true</showIcons>
            <components>
                <component>
                    <title>Login</title>
                    <description>Obtain Grid Credentials!!!</description>
                    <classname>org.cagrid.gaards.ui.dorian.ifs.CreateProxyWindow</classname>
                    <icon>/login.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>700</width>
                        <height>400</height>
                    </dimensions>
                </component>
                <component>
                    <title>Credential Manager</title>
                    <description>Allows the management of grid proxy credentials.</description>
                    <classname>org.cagrid.gaards.ui.common.CredentialManagerComponent</classname>
                    <icon>/certificate.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>800</width>
                        <height>500</height>
                    </dimensions>
                </component>
                 <component>
                    <title>My Groups</title>
                    <description>View the groups in which you are a member of.</description>
                    <classname>org.cagrid.gaards.ui.gridgrouper.mygroups.MyGroupsWindow</classname>
                    <icon>/grouper_logo_no_background_22x22.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>800</width>
                        <height>300</height>
                    </dimensions>
                </component>
                
                <component>
                    <title>Delegate Credential</title>
                    <description>Allows one to delegate their credential</description>
                    <classname>org.cagrid.gaards.ui.cds.DelegateProxyWindowStep1</classname>
                    <icon>/delegate-credential.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>600</width>
                        <height>350</height>
                    </dimensions>
                </component>
                
                <component>
                    <title>Get Delegated Credential</title>
                    <description>Allows one obtain a credential delegated to them.</description>
                    <classname>org.cagrid.gaards.ui.cds.GetDelegatedCredentialWindow</classname>
                    <icon>/delegated-credential.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>800</width>
                        <height>300</height>
                    </dimensions>
                </component>
                
                 <component>
                    <title>My Delegated Credentials</title>
                    <description>Allows one to view the credentials they delegated.</description>
                    <classname>org.cagrid.gaards.ui.cds.FindMyDelegatedCredentialsWindow</classname>
                    <icon>/delegate-credentials.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>800</width>
                        <height>500</height>
                    </dimensions>
                </component>
                   <component>
                    <title>Request Host Certificate</title>
                    <description>Allows a user to request a host ceritficate from Dorian.</description>
                    <classname>org.cagrid.gaards.ui.dorian.ifs.RequestHostCertificateWindow</classname>
                    <icon>/computer.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>500</width>
                        <height>350</height>
                    </dimensions>
                </component>
                 <component>
                    <title>My Host Certificates</title>
                    <description>Allows a user to obtain the host certificates in which they own.</description>
                    <classname>org.cagrid.gaards.ui.dorian.ifs.MyHostCertificatesWindow</classname>
                    <icon>/computers.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                </component>
                <component>
                    <title>Download CA Certificate(s)</title>
                    <description>Download the Dorian CA certificate.</description>
                    <classname>org.cagrid.gaards.ui.dorian.ifs.ViewCACertificateWindow</classname>
                    <icon>/download.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>625</width>
                        <height>200</height>
                    </dimensions>
                </component>
            </components>
        </menu>
        <menu>
           <title>Account Management</title>
            <description>tools</description>
            <showIcons>false</showIcons>
            <submenus>
 
              <menu>
            <title>Local Accounts</title>
            <description>Tools for registering and managing local Dorian accounts.</description>
            <showIcons>true</showIcons>
            <components>
                <component>
                    <title>Registration</title>
                    <description>Register with a Dorian for a local account.</description>
                    <classname>org.cagrid.gaards.ui.dorian.idp.ApplicationWindow</classname>
                    <icon>/application.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>700</width>
                        <height>650</height>
                    </dimensions>
                </component>
                   <component>
                    <title>Change Password</title>
                    <description>Allows a Dorian IdP user to change their password.</description>
                    <classname>org.cagrid.gaards.ui.dorian.idp.ChangePasswordWindow</classname>
                    <icon>/password.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>650</width>
                        <height>300</height>
                    </dimensions>
                </component>
                <component>
                    <title>Local Account Management</title>
                    <description>Local Account Management</description>
                    <classname>org.cagrid.gaards.ui.dorian.idp.UserManagerWindow</classname>
                    <icon>/users.png</icon>
                    <showOnToolBar>false</showOnToolBar>
                    <dimensions>
                        <width>800</width>
                        <height>650</height>
                    </dimensions>
                </component>

            </components>
        </menu>
            </submenus>
        </menu>
        
        </submenus>
        </menu>
    </menus>
    
    <configuration xmlns="http://www.cagrid.org/1/grape">
      <ConfigurationGroups>
        <ConfigurationGroup>
        <name>Security</name>
        <ConfigurationDescriptors>
            <ConfigurationDescriptor>
                <DisplayName>Grid User Management</DisplayName>
                <SystemName>dorian</SystemName>
                <ModelClassname>org.cagrid.grape.configuration.GeneralConfiguration</ModelClassname>
                <qname>
                    <namespace>http://www.cagrid.org/1/grape-general-configuration</namespace>
                    <name>GeneralConfiguration</name>
                </qname>
                <ConfigurationPanel>org.cagrid.grape.GeneralConfigurationEditor</ConfigurationPanel>
                <DefaultFile>extensions/cagrid_gaards_ui/dorian-ui-configuration.xml</DefaultFile>
            </ConfigurationDescriptor>
            <ConfigurationDescriptor>
                <DisplayName>Grid Trust Fabric</DisplayName>
                <SystemName>gts</SystemName>
                <ModelClassname>org.cagrid.grape.configuration.GeneralConfiguration</ModelClassname>
                <qname>
                    <namespace>http://www.cagrid.org/1/grape-general-configuration</namespace>
                    <name>GeneralConfiguration</name>
                </qname>
                <ConfigurationPanel>org.cagrid.grape.GeneralConfigurationEditor</ConfigurationPanel>
                <DefaultFile>extensions/cagrid_gaards_ui/gts-ui-configuration.xml</DefaultFile>
            </ConfigurationDescriptor>
            <ConfigurationDescriptor>
                <DisplayName>Group Management</DisplayName>
                <SystemName>gridgrouper</SystemName>
                <ModelClassname>org.cagrid.grape.configuration.GeneralConfiguration</ModelClassname>
                <qname>
                    <namespace>http://www.cagrid.org/1/grape-general-configuration</namespace>
                    <name>GeneralConfiguration</name>
                </qname>
                <ConfigurationPanel>org.cagrid.grape.GeneralConfigurationEditor</ConfigurationPanel>
                <DefaultFile>extensions/cagrid_gaards_ui/gridgrouper-ui-configuration.xml</DefaultFile>
            </ConfigurationDescriptor>
             <ConfigurationDescriptor>
                <DisplayName>Credential Delegation Service</DisplayName>
                <SystemName>cds</SystemName>
                <ModelClassname>org.cagrid.grape.configuration.GeneralConfiguration</ModelClassname>
                <qname>
                    <namespace>http://www.cagrid.org/1/grape-general-configuration</namespace>
                    <name>GeneralConfiguration</name>
                </qname>
                <ConfigurationPanel>org.cagrid.grape.GeneralConfigurationEditor</ConfigurationPanel>
                <DefaultFile>extensions/cagrid_gaards_ui/cds-ui-configuration.xml</DefaultFile>
            </ConfigurationDescriptor>
        </ConfigurationDescriptors>
        </ConfigurationGroup>
       </ConfigurationGroups>
    </configuration>
     <ns1:Properties>
     </ns1:Properties>
	</ns1:IntroduceGDEExtensionDescriptionType>
	<ns1:UpgradesDescription>
	</ns1:UpgradesDescription>
</ns1:ExtensionDescription>
