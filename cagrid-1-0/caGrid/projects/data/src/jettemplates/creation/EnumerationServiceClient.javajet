package <%=basePackage%>.client;

import java.rmi.RemoteException;

import org.apache.axis.message.addressing.EndpointReferenceType;
import org.apache.axis.types.URI.MalformedURIException;

import org.globus.gsi.GlobusCredential;

/**
 * This class is autogenerated by the caGrid Data Service extension for use
 * when the data service is used with WS-Enumeration
 */
 
public class <%=dataSourceClassName%> extends <%=serviceClientName%> implements <%=interfaceName%> {

	public <%=dataSourceClassName%>(String url) throws MalformedURIException, RemoteException {
		super(url, null);	
	}
	

	public <%=dataSourceClassName%>(String url, GlobusCredential proxy) throws MalformedURIException, RemoteException {
	   	super(url, proxy);
	}
	
	
	public <%=dataSourceClassName%>(EndpointReferenceType epr) throws MalformedURIException, RemoteException {
	   	super(epr, null);
	}
	
	
	public <%=dataSourceClassName%>(EndpointReferenceType epr, GlobusCredential proxy) throws MalformedURIException, RemoteException {
	   	super(epr, proxy);
	}
}