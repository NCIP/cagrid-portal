<% gov.nih.nci.cagrid.introduce.info.ServiceInformation serviceInfo = (gov.nih.nci.cagrid.introduce.info.ServiceInformation) argument;
		String serviceName = serviceInfo.getServices().getService()[0].getName();
		String basePackage = serviceInfo.getServices().getService()[0].getPackageName();
		String serviceClientName = basePackage + ".client." + serviceName + "Client";
		// name the new client class
		String dataSourceClassName = serviceName + "DataSourceClient";
		String interfaceName = org.xmlsoap.schemas.ws._2004._09.enumeration.DataSource.class.getName();
