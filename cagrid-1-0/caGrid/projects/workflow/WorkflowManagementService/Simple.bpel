<process name="Simple" suppressJoinFailure="yes" targetNamespace="http://Simple" xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"  xmlns:simple="http://workflow.cagrid.nci.nih.gov/SampleService1" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <partnerLinks>
      <partnerLink myRole="sample1" name="Sample1PartnerLinkType" partnerLinkType="simple:Sample1PartnerLinkType" partnerRole="sample1"/>
   </partnerLinks>
   <variables>
      <variable messageType="simple:invokeInputMessage" name="invokeInputMessage"/>
      <variable messageType="simple:invokeOutputMessage" name="invokeOutputMessage"/>
   </variables>
   <sequence>
      <receive createInstance="yes" operation="invoke" partnerLink="Sample1PartnerLinkType" portType="simple:SampleService1PortType" variable="invokeInputMessage"/>
      <invoke inputVariable="invokeInputMessage" operation="invoke" outputVariable="invokeOutputMessage" partnerLink="Sample1PartnerLinkType" portType="simple:SampleService1PortType"/>
      <reply operation="invoke" partnerLink="Sample1PartnerLinkType" portType="simple:SampleService1PortType" variable="invokeOutputMessage"/>
   </sequence>
</process>
