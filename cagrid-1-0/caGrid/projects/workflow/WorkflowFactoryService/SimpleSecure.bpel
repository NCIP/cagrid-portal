<process xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/" xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/" xmlns:ns1="http://sample1.tests.workflow.cagrid.nci.nih.gov/WorkflowTestService1" xmlns:ns2="http://cagrid.nci.nih.gov/SecureSample" xmlns:ns3="http://sample1.tests.workflow.cagrid.nci.nih.gov/SampleService1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="SimpleSecure" suppressJoinFailure="yes" targetNamespace="http://SimpleSecure">
   <partnerLinks>
      <partnerLink name="SecureSamplePartnerLinkType" partnerLinkType="ns2:SecureSamplePartnerLinkType" partnerRole="secure"/>
      <partnerLink myRole="test1" name="Test1PartnerLinkType" partnerLinkType="ns1:Test1PartnerLinkType" partnerRole="test1"/>
   </partnerLinks>
   <variables>
      <variable messageType="ns1:InvokeRequest" name="InvokeRequest"/>
      <variable messageType="ns1:InvokeResponse" name="InvokeResponse"/>
      <variable messageType="ns2:InvokeRequest" name="InvokeRequest1"/>
      <variable messageType="ns2:InvokeResponse" name="InvokeResponse1"/>
   </variables>
   <sequence>
      <receive createInstance="yes" operation="invoke" partnerLink="Test1PartnerLinkType" portType="ns1:WorkflowTestService1PortType" variable="InvokeRequest"/>
      <invoke inputVariable="InvokeRequest" operation="invoke" outputVariable="InvokeResponse" partnerLink="Test1PartnerLinkType" portType="ns1:WorkflowTestService1PortType"/>
      <assign>
         <copy>
            <from part="parameters" query="/ns1:InvokeResponse/ns3:response" variable="InvokeResponse"/>
            <to part="parameters" query="/ns2:InvokeRequest/ns2:string" variable="InvokeRequest1"/>
         </copy>
      </assign>
      <invoke inputVariable="InvokeRequest1" operation="invoke" outputVariable="InvokeResponse1" partnerLink="SecureSamplePartnerLinkType" portType="ns2:SecureSamplePortType"/>
      <reply operation="invoke" partnerLink="Test1PartnerLinkType" portType="ns1:WorkflowTestService1PortType" variable="InvokeResponse"/>
   </sequence>
</process>
