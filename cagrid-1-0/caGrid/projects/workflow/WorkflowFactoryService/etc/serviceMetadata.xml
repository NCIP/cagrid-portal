<ns1:ServiceMetadata xmlns:ns1="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata">
 <ns1:serviceDescription>
  <ns2:Service description="The WorkflowFactoryService grid service, created with caGrid Introduce, version:1.0. This service is used to execute BPEL workflows that orchestrate caGrid Analytical and Data Services." name="WorkflowFactoryService" version="1.0"
   xmlns:ns2="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.service">
   <ns2:pointOfContactCollection>
    <ns3:PointOfContact affiliation="UC" email="madduri@mcs.anl.gov" firstName="Ravi" lastName="Madduri" role="Developer" xmlns:ns3="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.common"
    />
   </ns2:pointOfContactCollection>
   <ns2:serviceContextCollection>
    <ns2:ServiceContext description="Workflow Factory Service to create workflow resources" name="WorkflowFactoryService">
     <ns2:operationCollection>
      <ns2:Operation description="Get the service security metadata for the Worfkflow factory service" name="getServiceSecurityMetadata">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns3:ServiceSecurityMetadata" dimensionality="1" isArray="false"
        xmlns:ns3="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"/>
       <ns2:faultCollection/>
      </ns2:Operation>
      <ns2:Operation description="creating workflow resources" name="createWorkflow">
       <ns2:inputParameterCollection>
        <ns2:InputParameter qName="ns4:WMSInputElement" dimensionality="1" index="0" isArray="false" isRequired="false" name="wMSInputElement"
         xmlns:ns4="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       </ns2:inputParameterCollection>
       <ns2:Output qName="ns5:WMSOutputElement" dimensionality="1" isArray="false"
        xmlns:ns5="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="Workflow Exception" name="WorkflowException"/>
       </ns2:faultCollection>
      </ns2:Operation>
     </ns2:operationCollection>
     <ns2:contextPropertyCollection>
      <ns2:ContextProperty description="Service Metadata" name="{gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata}ServiceMetadata"/>
     </ns2:contextPropertyCollection>
    </ns2:ServiceContext>
    <ns2:ServiceContext description="Workflow Service that will let users start workflows, get status, pause, resume and get intermediate results" name="WorkflowServiceImpl">
     <ns2:operationCollection>
      <ns2:Operation description="Get Security Metadata of the Workflow Service" name="getServiceSecurityMetadata">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns6:ServiceSecurityMetadata" dimensionality="1" isArray="false"
        xmlns:ns6="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"/>
       <ns2:faultCollection/>
      </ns2:Operation>
      <ns2:Operation description="Start the workflows with the input data" name="start">
       <ns2:inputParameterCollection>
        <ns2:InputParameter qName="ns7:StartInputElement" dimensionality="1" index="0" isArray="false" isRequired="false" name="startInputElement"
         xmlns:ns7="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       </ns2:inputParameterCollection>
       <ns2:Output qName="ns8:WorkflowStatusElement" dimensionality="1" isArray="false"
        xmlns:ns8="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="Workflow Exception" name="WorkflowException"/>
        <ns2:Fault description="This Exception is thrown when start is called on already started workflows" name="StartCalledOnStartedWorkflowFault"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="Get Status of the workflows" name="getStatus">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns9:WorkflowStatusElement" dimensionality="1" isArray="false"
        xmlns:ns9="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="Pause the active workflows" name="pause">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns10:WorkflowStatusElement" dimensionality="1" isArray="false"
        xmlns:ns10="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
        <ns2:Fault description="This is thrown when Workflow cannot be paused" name="CannotPauseWorkflowFault"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="Resumes paused Workflows" name="resume">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns11:WorkflowStatusElement" dimensionality="1" isArray="false"
        xmlns:ns11="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
        <ns2:Fault description="This exception is thrown when a workflow cannot resumed" name="CannotResumeWorkflowFault"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="Cancel the workflow" name="cancel">
       <ns2:inputParameterCollection/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
        <ns2:Fault description="This exception is thrown when a workflow cannot be cancelled" name="CannotCancelWorkflowFault"/>
       </ns2:faultCollection>
      </ns2:Operation>
      <ns2:Operation description="This operation is used to get intermediate output from the workflows" name="getWorkflowOutput">
       <ns2:inputParameterCollection/>
       <ns2:Output qName="ns12:WorkflowOutputElement" dimensionality="1" isArray="false"
        xmlns:ns12="http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService"/>
       <ns2:faultCollection>
        <ns2:Fault description="" name="WorkflowException"/>
       </ns2:faultCollection>
      </ns2:Operation>
     </ns2:operationCollection>
     <ns2:contextPropertyCollection>
      <ns2:ContextProperty description="Workflow Status type" name="{http://types.workflow.cagrid.nci.nih.gov/WorkflowManagementFactoryService}WorkflowStatusElement"/>
     </ns2:contextPropertyCollection>
    </ns2:ServiceContext>
   </ns2:serviceContextCollection>
   <ns39:SemanticMetadata conceptCode="C63541"
    conceptDefinition="A service available on a system that coordinates resources that are not subject to central control, using open, general purpose protocols and interfaces to deliver nontrivial qualities of services."
    conceptName="Grid Service" order="0" xmlns:ns39="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.common"/>
   <ns40:SemanticMetadata conceptCode="C63542"
    conceptDefinition="Services that are needed to maintain the running of a grid, including monitoring and discovery service, security management service (authentication and authorization), identifier naming and resolution service, query and federated query service, and workflow management service."
    conceptName="Core Grid Service" order="1" xmlns:ns40="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.common"/>
  </ns2:Service>
 </ns1:serviceDescription>
 <ns1:hostingResearchCenter>
  <ns69:ResearchCenter displayName="University of Chicago" shortName="UC" xmlns:ns69="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.common">
   <ns69:Address country="USA" postalCode="60637" stateProvince="IL" street1="9700 S Cass Avenue"/>
   <ns69:pointOfContactCollection>
    <ns69:PointOfContact affiliation="University of Chicago" email="madduri@mcs.anl.gov" firstName="Ravi" lastName="Madduri" role="maintainer"/>
   </ns69:pointOfContactCollection>
  </ns69:ResearchCenter>
 </ns1:hostingResearchCenter>
</ns1:ServiceMetadata>
