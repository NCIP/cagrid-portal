<project name="portal2" default="all">

	<target name="all">
		<ant dir="common" target="all"/>
		<ant dir="webapps/cagridportlets" target="all"/>
		<ant dir="services/webauthn" target="jars"/>
		<ant dir="portals/jetspeed" target="all"/>
	</target>
	
	<target name="test">
		<ant dir="common" target="test" inheritAll="false" inheritRefs="false">
			<property name="db" value="test"/>
			<property name="data.file.prefix" value="common/"/>
		</ant>
	</target>
	
	<target name="clean">
		<ant dir="common" target="clean"/>
		<ant dir="webapps/cagridportlets" target="clean"/>
		<ant dir="services/webauthn" target="clean"/>
		<ant dir="portals/jetspeed" target="clean"/>	
	</target>
	
</project>
