 <ns3:Services xsi:type="ns3:ServicesType" xmlns:ns3="gme://gov.nih.nci.cagrid.introduce/1/Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns3:Service name="WebAuthnSvc" namespace="http://webauthn.portal2.cagrid.nci.nih.gov/WebAuthnSvc" packageName="gov.nih.nci.cagrid.portal2.webauthn" resourceFrameworkType="main" xsi:type="ns3:ServiceType">
   <ns4:Methods xsi:type="ns4:MethodsType" xmlns:ns4="gme://gov.nih.nci.cagrid.introduce/1/Methods">
    <ns4:Method isImported="true" isProvided="true" name="getServiceSecurityMetadata" xsi:type="ns4:MethodType">
     <ns4:Output qName="ns5:ServiceSecurityMetadata" isArray="false" isClientHandle="false" xmlns:ns5="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"/>
     <ns4:ImportInformation namespace="http://security.introduce.cagrid.nci.nih.gov/ServiceSecurity" packageName="gov.nih.nci.cagrid.introduce.security.stubs" portTypeName="ServiceSecurityPortType" wsdlFile="ServiceSecurity.wsdl"/>
     <ns4:ProviderInformation providerClass="gov.nih.nci.cagrid.introduce.security.service.globus.ServiceSecurityProviderImpl"/>
    </ns4:Method>
    <ns4:Method description="Returns username, roles and, optionally, a SAMLAssertion." isImported="false" isProvided="false" name="getUserInfo" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="xsd:string" description="Single-use key which references UserInfo." isArray="false" name="loginKey" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
     </ns4:Inputs>
     <ns4:Output qName="ns6:UserInfo" description="" isArray="false" xmlns:ns6="http://cagrid.nci.nih.gov/1/webauthn"/>
     <ns4:Exceptions>
      <ns4:Exception description="Returned if key does not map to a UserInfo object. This could mean that the key is expired, has already been used, or was never valid." name="InvalidKeyFault" qname="ns7:InvalidKeyFault" xmlns:ns7="http://webauthn.portal2.cagrid.nci.nih.gov/WebAuthnSvc/types"/>
     </ns4:Exceptions>
    </ns4:Method>
    <ns4:Method description="Requests that a login key be created, given user credentials." isImported="false" isProvided="false" name="createLoginKeyForLocalUser" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns8:Credential" description="" isArray="false" name="credential" xmlns:ns8="http://cagrid.nci.nih.gov/1/authentication-service"/>
     </ns4:Inputs>
     <ns4:Output qName="xsd:string" description="The loginKey" isArray="false" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
     <ns4:Exceptions>
      <ns4:Exception description="" name="InvalidCredentialFault" qname="ns9:InvalidCredentialFault" xmlns:ns9="http://cagrid.nci.nih.gov/AuthenticationService/types"/>
      <ns4:Exception description="" name="InsufficientAttributeFault" qname="ns10:InsufficientAttributeFault" xmlns:ns10="http://cagrid.nci.nih.gov/AuthenticationService/types"/>
      <ns4:Exception description="" name="AuthenticationProviderFault" qname="ns11:AuthenticationProviderFault" xmlns:ns11="http://cagrid.nci.nih.gov/AuthenticationService/types"/>
      <ns4:Exception description="Thrown if there is an internal error in the WebAuthnSvc service." name="WebAuthnSvcFault" qname="ns12:WebAuthnSvcFault" xmlns:ns12="http://webauthn.portal2.cagrid.nci.nih.gov/WebAuthnSvc/types"/>
     </ns4:Exceptions>
    </ns4:Method>
    <ns4:Method description="" isImported="false" isProvided="false" name="createLoginKeyForGridUser" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="xsd:string" description="" isArray="false" name="idpUrl" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
      <ns4:Input qName="ns13:Credential" description="" isArray="false" name="credential" xmlns:ns13="http://cagrid.nci.nih.gov/1/authentication-service"/>
     </ns4:Inputs>
     <ns4:Output qName="xsd:string" description="" isArray="false" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
     <ns4:Exceptions>
      <ns4:Exception description="" name="InvalidCredentialFault" qname="ns14:InvalidCredentialFault" xmlns:ns14="http://cagrid.nci.nih.gov/AuthenticationService/types"/>
      <ns4:Exception description="" name="InsufficientAttributeFault" qname="ns15:InsufficientAttributeFault" xmlns:ns15="http://cagrid.nci.nih.gov/AuthenticationService/types"/>
      <ns4:Exception description="" name="AuthenticationProviderFault" qname="ns16:AuthenticationProviderFault" xmlns:ns16="http://cagrid.nci.nih.gov/AuthenticationService/types"/>
      <ns4:Exception description="" name="WebAuthnSvcFault" qname="ns17:WebAuthnSvcFault" xmlns:ns17="http://webauthn.portal2.cagrid.nci.nih.gov/WebAuthnSvc/types"/>
     </ns4:Exceptions>
    </ns4:Method>
   </ns4:Methods>
   <ns18:ResourcePropertiesList xsi:type="ns18:ResourcePropertiesListType" xmlns:ns18="gme://gov.nih.nci.cagrid.introduce/1/Resources">
    <ns18:ResourceProperty qName="ns19:ServiceMetadata" description="" fileLocation="serviceMetadata.xml" populateFromFile="true" register="true" xsi:type="ns18:ResourcePropertyType" xmlns:ns19="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"/>
   </ns18:ResourcePropertiesList>
   <ns3:description xsi:type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema"></ns3:description>
  </ns3:Service>
