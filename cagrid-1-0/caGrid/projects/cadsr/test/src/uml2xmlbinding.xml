<?xml version="1.0" encoding="UTF-8"?>
<UML2XMLBindingConfiguration xmlns="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.cadsr.uml2xml.file.configuration">
    <Namespaces>
        <!-- Each schema which is used to represent registered datatypes should
             have an entry here
        -->
        <Namespace namespace="gme://caCORE.caCORE/3.1/gov.nih.nci.cabio.domain">
            <ElementBindings>
                <!-- Specific Elements could be listed here -->
                <Element elementName="GeneFoo" umlClassName="Gene">
                    <UMLPackageBinding package="gov.nih.nci.cabio.domain" projectContext="caCORE" projectShortName="caCORE 3.1" projectVersion="3.1"/>
                </Element>
            </ElementBindings>
            <DefaultUMLPackageBinding>
                <!-- When an element isn't listed above, 
                     the element name and class name will be assumed to be the same,
                     and this information will be used to map to the 
                     package and project
                 -->
                <UMLPackageBinding package="gov.nih.nci.cabio.domain" projectContext="caCORE" projectShortName="caCORE 3.1" projectVersion="3.1"/>
            </DefaultUMLPackageBinding>
        </Namespace>
    </Namespaces>
</UML2XMLBindingConfiguration>
