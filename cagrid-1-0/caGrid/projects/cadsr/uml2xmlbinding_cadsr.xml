<?xml version="1.0" encoding="UTF-8"?>
<UML2XMLBindingConfiguration xmlns="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.cadsr.uml2xml.file.configuration">
    <!-- This is an example configuration file that can be used with the file based serviceMetadataAnnotation
         program.  This file can be used for the caDSR grid service itself.  It defines the bindings for the 
         two caDSR-registered packages from which the caDSR grid service uses types for inputs and outputs.
      -->
    <Namespaces>
        <!-- Each schema which is used to represent registered datatypes should
             have an entry here
        -->
        <Namespace namespace="gme://caCORE.caCORE/3.1/gov.nih.nci.cadsr.domain">
            <DefaultUMLPackageBinding>
                <!-- When an element isn't listed above, 
                     the element name and class name will be assumed to be the same,
                     and this information will be used to map to the 
                     package and project
                 -->
                <UMLPackageBinding package="gov.nih.nci.cadsr.domain" projectContext="caCORE" projectShortName="caCORE 3.1" projectVersion="3.1"/>
            </DefaultUMLPackageBinding>
        </Namespace>
        
        <Namespace namespace="gme://caCORE.caCORE/3.1/gov.nih.nci.cadsr.umlproject.domain">
            <DefaultUMLPackageBinding>
                <!-- When an element isn't listed above, 
                    the element name and class name will be assumed to be the same,
                    and this information will be used to map to the 
                    package and project
                -->
                <UMLPackageBinding package="gov.nih.nci.cadsr.umlproject.domain" projectContext="caCORE" projectShortName="caCORE 3.1" projectVersion="3.1"/>
            </DefaultUMLPackageBinding>
        </Namespace>
        
        
    </Namespaces>
</UML2XMLBindingConfiguration>
