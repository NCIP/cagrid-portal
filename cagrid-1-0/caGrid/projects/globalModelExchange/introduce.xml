<ns1:ServiceDescription introduceVersion="1.1" xmlns:ns1="gme://gov.nih.nci.cagrid/1/Introduce">
 <ns2:Namespaces xsi:type="ns2:NamespacesType" xmlns:ns2="gme://gov.nih.nci.cagrid.introduce/1/Namespace" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns2:Namespace generateStubs="false" location="./xsd/cagrid/types/caGridMetadata.xsd" namespace="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata" packageName="gov.nih.nci.cagrid.metadata" xsi:type="ns2:NamespaceType">
   <ns2:SchemaElement type="ServiceMetadata" xsi:type="ns2:SchemaElementType"/>
  </ns2:Namespace>
  <ns2:Namespace location="./gme.xsd" namespace="gme://gme.cagrid.org/1/GlobalModelExchange" packageName="org.cagrid.gme.protocol.stubs" xsi:type="ns2:NamespaceType">
   <ns2:SchemaElement type="Schema" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="Namespace" xsi:type="ns2:SchemaElementType"/>
  </ns2:Namespace>
  <ns2:Namespace location="./schema.xsd" namespace="http://www.w3.org/2001/XMLSchema" packageName="xmlschema" xsi:type="ns2:NamespaceType">
   <ns2:SchemaElement type="simpleType" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="restriction" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="list" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="union" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="minExclusive" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="minInclusive" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="maxExclusive" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="maxInclusive" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="totalDigits" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="fractionDigits" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="length" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="minLength" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="maxLength" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="enumeration" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="whiteSpace" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="pattern" xsi:type="ns2:SchemaElementType"/>
  </ns2:Namespace>
  <ns2:Namespace location="./GlobalModelExchangeTypes.xsd" namespace="http://gme.cagrid.org/GlobalModelExchange/types" packageName="org.cagrid.gme.stubs.types" xsi:type="ns2:NamespaceType">
   <ns2:SchemaElement type="GlobalModelExchangeReference" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="SchemaAlreadyExists" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="InvalidSchemaSubmission" xsi:type="ns2:SchemaElementType"/>
  </ns2:Namespace>
  <ns2:Namespace generateStubs="false" location="./xsd/cagrid/types/security/security.xsd" namespace="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security" packageName="gov.nih.nci.cagrid.metadata.security" xsi:type="ns2:NamespaceType">
   <ns2:SchemaElement type="ServiceSecurityMetadata" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="CommunicationMechanism" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="Operation" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="CommunicationStyle" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="GSISecureMessage" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="GSISecureConversation" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="GSITransport" xsi:type="ns2:SchemaElementType"/>
   <ns2:SchemaElement type="None" xsi:type="ns2:SchemaElementType"/>
  </ns2:Namespace>
 </ns2:Namespaces>
 <ns3:Services xsi:type="ns3:ServicesType" xmlns:ns3="gme://gov.nih.nci.cagrid.introduce/1/Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns3:Service name="GlobalModelExchange" namespace="http://gme.cagrid.org/GlobalModelExchange" packageName="org.cagrid.gme" resourceFrameworkType="main" xsi:type="ns3:ServiceType">
   <ns4:Methods xsi:type="ns4:MethodsType" xmlns:ns4="gme://gov.nih.nci.cagrid.introduce/1/Methods">
    <ns4:Method isImported="true" isProvided="true" name="getServiceSecurityMetadata" xsi:type="ns4:MethodType">
     <ns4:Output qName="ns5:ServiceSecurityMetadata" isArray="false" isClientHandle="false" xmlns:ns5="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"/>
     <ns4:ImportInformation namespace="http://security.introduce.cagrid.nci.nih.gov/ServiceSecurity" packageName="gov.nih.nci.cagrid.introduce.security.stubs" portTypeName="ServiceSecurityPortType" wsdlFile="ServiceSecurity.wsdl"/>
     <ns4:ProviderInformation providerClass="gov.nih.nci.cagrid.introduce.security.service.globus.ServiceSecurityProviderImpl"/>
    </ns4:Method>
    <ns4:Method description="Adds or updates a set of schemas" isImported="false" isProvided="false" name="addSchemas" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns6:Schema" description="the schemas to add or update" isArray="true" name="schemas" xmlns:ns6="gme://gme.cagrid.org/1/GlobalModelExchange"/>
     </ns4:Inputs>
     <ns4:Output qName="void" description="" isArray="false"/>
     <ns4:Exceptions>
      <ns4:Exception description="" name="SchemaAlreadyExists" qname="ns7:SchemaAlreadyExists" xmlns:ns7="http://gme.cagrid.org/GlobalModelExchange/types"/>
      <ns4:Exception description="" name="InvalidSchemaSubmission" qname="ns8:InvalidSchemaSubmission" xmlns:ns8="http://gme.cagrid.org/GlobalModelExchange/types"/>
     </ns4:Exceptions>
    </ns4:Method>
    <ns4:Method description="Returns all the namespaces the GME knows about" isImported="false" isProvided="false" name="getNamespaces" xsi:type="ns4:MethodType">
     <ns4:Inputs/>
     <ns4:Output qName="ns9:Namespace" description="" isArray="true" xmlns:ns9="gme://gme.cagrid.org/1/GlobalModelExchange"/>
     <ns4:Exceptions/>
    </ns4:Method>
   </ns4:Methods>
   <ns10:ResourcePropertiesList xsi:type="ns10:ResourcePropertiesListType" xmlns:ns10="gme://gov.nih.nci.cagrid.introduce/1/Resources">
    <ns10:ResourceProperty qName="ns11:ServiceMetadata" description="caGrid standard Service Metadata" fileLocation="serviceMetadata.xml" populateFromFile="true" register="true" xsi:type="ns10:ResourcePropertyType" xmlns:ns11="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"/>
   </ns10:ResourcePropertiesList>
   <ns3:description xsi:type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
        The Global Model Exchange (GME) acts as an authoritative source
        for XML Schemas, providing a robust type management system for
        use in Grids and other XML-based environments.
      </ns3:description>
  </ns3:Service>
 </ns3:Services>
 <ns12:Extensions xsi:type="ns12:ExtensionsType" xmlns:ns12="gme://gov.nih.nci.cagrid.introduce/1/Extension" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns12:Extension name="cagrid_metadata" version="1.1" xsi:type="ns12:ExtensionType"/>
 </ns12:Extensions>
 <ns13:ServiceProperties xmlns:ns13="gme://gov.nih.nci.cagrid.introduce/1/Property">
  <ns13:Property description="" isFromETC="false" key="schemaPersistenceImplementationClassname" value="org.cagrid.gme.persistence.torque.TorqueSchemaPersistenceImpl"/>
  <ns13:Property description="" isFromETC="true" key="schemaPersistenceImplementationPropertiesFile" value="torque.properties"/>
 </ns13:ServiceProperties>
</ns1:ServiceDescription>
