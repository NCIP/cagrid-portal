  <ns23:Property description="The location (relative to the service's ETC location) of the XML configuration file" isFromETC="true" key="gmeConfigurationFile" value="applicationContext-gme.xml"/>
  <ns23:Property description="The location (relative to the service's ETC location) of the properties file containing information about the database and other settings" isFromETC="true" key="gmePropertiesFile" value="gme.properties"/>
 </ns23:ServiceProperties>
</ns1:ServiceDescription>
