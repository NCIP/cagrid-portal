 <ns3:Services xsi:type="ns3:ServicesType" xmlns:ns3="gme://gov.nih.nci.cagrid.introduce/1/Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns3:Service name="GlobalModelExchange" namespace="http://gme.cagrid.org/GlobalModelExchange" packageName="org.cagrid.gme" xsi:type="ns3:ServiceType">
   <ns4:Methods xsi:type="ns4:MethodsType" xmlns:ns4="gme://gov.nih.nci.cagrid.introduce/1/Methods">
    <ns4:Method isImported="true" isProvided="true" name="getServiceSecurityMetadata" xsi:type="ns4:MethodType">
     <ns4:Output qName="ns5:ServiceSecurityMetadata" isArray="false" isClientHandle="false" xmlns:ns5="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"/>
     <ns4:ImportInformation namespace="http://security.introduce.cagrid.nci.nih.gov/ServiceSecurity" packageName="gov.nih.nci.cagrid.introduce.security.stubs" portTypeName="ServiceSecurityPortType" wsdlFile="ServiceSecurity.wsdl"/>
     <ns4:ProviderInformation providerClass="gov.nih.nci.cagrid.introduce.security.service.globus.ServiceSecurityProviderImpl"/>
    </ns4:Method>
    <ns4:Method isImported="true" isProvided="true" name="GetMultipleResourceProperties" xsi:type="ns4:MethodType">
     <ns4:Output qName="void" isArray="false"/>
     <ns4:ImportInformation fromIntroduce="false" inputMessage="ns6:GetMultipleResourcePropertiesRequest" namespace="http://docs.oasis-open.org/wsrf/2004/06/wsrf-WS-ResourceProperties-1.2-draft-01.wsdl" outputMessage="ns6:GetMultipleResourcePropertiesResponse" packageName="org.oasis.wsrf.properties" portTypeName="GetMultipleResourceProperties" wsdlFile="../wsrf/properties/WS-ResourceProperties.wsdl" xmlns:ns6="http://docs.oasis-open.org/wsrf/2004/06/wsrf-WS-ResourceProperties-1.2-draft-01.wsdl"/>
     <ns4:ProviderInformation providerClass="GetMRPProvider"/>
    </ns4:Method>
    <ns4:Method isImported="true" isProvided="true" name="GetResourceProperty" xsi:type="ns4:MethodType">
     <ns4:Output qName="void" isArray="false"/>
     <ns4:ImportInformation fromIntroduce="false" inputMessage="ns7:GetResourcePropertyRequest" namespace="http://docs.oasis-open.org/wsrf/2004/06/wsrf-WS-ResourceProperties-1.2-draft-01.wsdl" outputMessage="ns7:GetResourcePropertyResponse" packageName="org.oasis.wsrf.properties" portTypeName="GetResourceProperty" wsdlFile="../wsrf/properties/WS-ResourceProperties.wsdl" xmlns:ns7="http://docs.oasis-open.org/wsrf/2004/06/wsrf-WS-ResourceProperties-1.2-draft-01.wsdl"/>
     <ns4:ProviderInformation providerClass="GetRPProvider"/>
    </ns4:Method>
    <ns4:Method isImported="true" isProvided="true" name="QueryResourceProperties" xsi:type="ns4:MethodType">
     <ns4:Output qName="void" isArray="false"/>
     <ns4:ImportInformation fromIntroduce="false" inputMessage="ns8:QueryResourcePropertiesRequest" namespace="http://docs.oasis-open.org/wsrf/2004/06/wsrf-WS-ResourceProperties-1.2-draft-01.wsdl" outputMessage="ns8:QueryResourcePropertiesResponse" packageName="org.oasis.wsrf.properties" portTypeName="QueryResourceProperties" wsdlFile="../wsrf/properties/WS-ResourceProperties.wsdl" xmlns:ns8="http://docs.oasis-open.org/wsrf/2004/06/wsrf-WS-ResourceProperties-1.2-draft-01.wsdl"/>
     <ns4:ProviderInformation providerClass="QueryRPProvider"/>
    </ns4:Method>
    <ns4:Method description="Publishes the provided schemas, replacing existing versions as necessary." isImported="false" isProvided="false" name="publishXMLSchemas" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns9:XMLSchema" description="The schemas to publish" isArray="true" name="schemas" xmlns:ns9="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     </ns4:Inputs>
     <ns4:Output qName="void" description="" isArray="false"/>
     <ns4:Exceptions/>
    </ns4:Method>
    <ns4:Method description="Returns a single XML Schema, not including any referenced (i.e. imported) XML Schemas" isImported="false" isProvided="false" name="getXMLSchema" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns10:XMLSchemaNamespace" description="A namespace representing the targetNamespace of the desired XMLSchema" isArray="false" name="targetNamespace" xmlns:ns10="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     </ns4:Inputs>
     <ns4:Output qName="ns11:XMLSchema" description="The XMLSchema with a targetNamespace equal to the given XMLSchemaNamespace" isArray="false" xmlns:ns11="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     <ns4:Exceptions/>
    </ns4:Method>
    <ns4:Method description="Returns the targetNamespaces (represented by XMLSchemaNamespaces) of all published XMLSchemas" isImported="false" isProvided="false" name="getXMLSchemaNamespaces" xsi:type="ns4:MethodType">
     <ns4:Inputs/>
     <ns4:Output qName="ns12:XMLSchemaNamespace" description="the targetNamespaces (represented by XMLSchemaNamespaces) of all published XMLSchemas " isArray="false" xmlns:ns12="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     <ns4:Exceptions/>
    </ns4:Method>
    <ns4:Method description="Returns an XMLSchemaBundle containing the transitive closure of the XMLSchema with the targetNamespace of the given XMLSchemaNamespace and every XMLSchema it imports (as well as each imported XMLSchema's imports)" isImported="false" isProvided="false" name="getXMLSchemaAndDependencies" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns13:XMLSchemaNamespace" description="The targetNamespace of the XMLSchema of interest" isArray="false" name="targetNamespace" xmlns:ns13="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     </ns4:Inputs>
     <ns4:Output qName="ns14:XMLSchemaBundle" description="an XMLSchemaBundle containing the transitive closure of the XMLSchema with the targetNamespace of the given XMLSchemaNamespace and every XMLSchema it imports (as well as each imported XMLSchema's imports)" isArray="false" xmlns:ns14="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     <ns4:Exceptions/>
    </ns4:Method>
    <ns4:Method description="Deletes the XMLSchemas with targetNamespaces identified by the given XMLSchemaNamespace array" isImported="false" isProvided="false" name="deleteXMLSchemas" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns15:XMLSchemaNamespace" description="The targetNamespaces of the XMLSchemas to delete" isArray="true" name="targetNamespaces" xmlns:ns15="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     </ns4:Inputs>
     <ns4:Output qName="void" description="" isArray="false"/>
     <ns4:Exceptions/>
    </ns4:Method>
    <ns4:Method description="Returns the targetNamespaces of all the XMLSchemas which are imported by the given XMLSchema (identififed by its targetNamespace)" isImported="false" isProvided="false" name="getImportedXMLSchemaNamespaces" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns16:XMLSchemaNamespace" description="The targetNamespace of the XMLSchema of interest" isArray="false" name="targetNamespace" xmlns:ns16="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     </ns4:Inputs>
     <ns4:Output qName="ns17:XMLSchemaNamespace" description="The imported XMLSchemas' targetNamespaces" isArray="true" xmlns:ns17="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     <ns4:Exceptions/>
    </ns4:Method>
    <ns4:Method description="Returns all the targetNamespaces of the XMLSchemas which import the given XMLSchema, identified its targetNamespace" isImported="false" isProvided="false" name="getImportingXMLSchemaNamespaces" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns18:XMLSchemaNamespace" description="The targetNamespace of the XMLSchema of interest" isArray="false" name="targetNamespace" xmlns:ns18="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     </ns4:Inputs>
     <ns4:Output qName="ns19:XMLSchemaNamespace" description="all the targetNamespaces of the XMLSchemas which import the given XMLSchema, identified its targetNamespace" isArray="true" xmlns:ns19="gme://gme.cagrid.org/2.0/GlobalModelExchange/domain"/>
     <ns4:Exceptions/>
    </ns4:Method>
   </ns4:Methods>
   <ns20:ResourcePropertiesList xsi:type="ns20:ResourcePropertiesListType" xmlns:ns20="gme://gov.nih.nci.cagrid.introduce/1/Resources">
    <ns20:ResourceProperty qName="ns21:ServiceMetadata" description="caGrid standard Service Metadata" fileLocation="serviceMetadata.xml" populateFromFile="true" register="true" xsi:type="ns20:ResourcePropertyType" xmlns:ns21="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"/>
   </ns20:ResourcePropertiesList>
   <ns3:description xsi:type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema">The Global Model Exchange (GME) acts as an authoritative source
        for XML Schemas, providing a robust type management system for
        use in Grids and other XML-based environments.</ns3:description>
   <ns3:ResourceFrameworkOptions xsi:type="ns3:ResourceFrameworkOptions">
    <ns3:Main xsi:type="ns3:Main"/>
    <ns3:Singleton xsi:type="ns3:Singleton"/>
    <ns3:Identifiable xsi:type="ns3:Identifiable"/>
    <ns3:Secure xsi:type="ns3:Secure"/>
    <ns3:ResourcePropertyManagement xsi:type="ns3:ResourcePropertyManagement"/>
   </ns3:ResourceFrameworkOptions>
  </ns3:Service>
 </ns3:Services>
