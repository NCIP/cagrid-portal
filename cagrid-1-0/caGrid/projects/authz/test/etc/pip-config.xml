<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="pip" class="gov.nih.nci.cagrid.authorization.impl.GridGrouperPIP">
		<property name="grouper">
			<bean class="gov.nih.nci.cagrid.gridgrouper.client.GridGrouper">
				<constructor-arg value="https://localhost:18443/wsrf/services/cagrid/GridGrouper"/>
			</bean>
		</property>
		<property name="searchStems">
			<set>
				<value>mystem</value>
			</set>
		</property>
	</bean>
</beans>
