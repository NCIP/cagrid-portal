<?xml version="1.0"?>
<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
>
  <info organisation="caGrid" module="globalModelExchange-ui" revision="${project.version}">
    <license name="caGrid" url="http://cagrid.org/wiki/CaGrid:LICENSE-1.3"/>
    <description>The caGrid Global Model Exchange (GME) User Interface project</description>
  </info>
  <configurations>
    <conf name="impl" visibility="private" extends="default" />
    <conf name="impl-test" visibility="private" />
    <conf name="target_grid" visibility="private" />
    
    <!-- Public Configurations-->
    <conf name="default" visibility="public" />
    <conf name="test" visibility="public" extends="default,impl-test" />
  </configurations>
  <publications>
    <artifact name="caGrid-${ivy.module}" conf="default" type="jar" />
    <artifact name="caGrid-${ivy.module}-tests" conf="test" type="jar" />
  </publications>
  <dependencies>
    <dependency rev="${target.grid}" org="caGrid" name="target_grid" conf="target_grid->urls" />
    
    <dependency rev="latest.integration" org="caGrid" name="globalModelExchange" conf="default->client" />
    <dependency rev="latest.integration" org="caGrid" name="introduce" conf="default->extension" />
    <dependency rev="latest.integration" org="caGrid" name="grape" conf="default->default" />
    <dependency rev="2.0.0" org="jgoodies" name="validation" conf="default->default" />
    <dependency rev="4.4" org="junit" name="junit" conf="impl-test->default"/>
    
  </dependencies>
</ivy-module>