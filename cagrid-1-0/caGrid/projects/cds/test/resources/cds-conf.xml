<?xml version="1.0" encoding="UTF-8"?>
<CDSConfiguration xmlns="http://gaards.cagrid.org/cds/conf"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <CDSInternalId>test_cds</CDSInternalId>
   <DatabaseConfiguration>
        <host>localhost</host>
        <port>3306</port>
        <username>root</username>
        <password></password>
    </DatabaseConfiguration>
      <ProxyPolicy>
    	<PermittedKeyLengths>
    		<KeyLength>1025</KeyLength>
    		<KeyLength>2056</KeyLength>
    	</PermittedKeyLengths>
    </ProxyPolicy>
    <KeyManagerDescription className="org.cagrid.gaards.cds.service.DBKeyManager">
    	<Property name="key-encryption-password" value="password"/>
    </KeyManagerDescription>
    <PolicyHandlers>
    	<PolicyHandlerDescription className="org.cagrid.gaards.cds.service.IdentityPolicyHandler" policyClassName="org.cagrid.gaards.cds.common.IdentityDelegationPolicy"/>
    </PolicyHandlers>
</CDSConfiguration>


