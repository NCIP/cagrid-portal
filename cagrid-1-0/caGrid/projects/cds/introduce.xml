    </ns12:TransportLevelSecurity>
    <ns12:RunAsMode xsi:type="ns12:RunAsMode">System</ns12:RunAsMode>
    <ns12:AnonymousClients xsi:type="ns12:AnonymousCommunication">No</ns12:AnonymousClients>
    <ns12:ServiceAuthorization xsi:type="ns12:ServiceAuthorization">
     <ns12:NoAuthorization xsi:type="ns12:NoAuthorization"/>
    </ns12:ServiceAuthorization>
   </ns12:ServiceSecurity>
