    <ns18:TransportLevelSecurity xsi:type="ns18:TransportLevelSecurity">
     <ns18:CommunicationMethod xsi:type="ns18:CommunicationMethod">Privacy</ns18:CommunicationMethod>
    </ns18:TransportLevelSecurity>
    <ns18:RunAsMode xsi:type="ns18:RunAsMode">System</ns18:RunAsMode>
    <ns18:AnonymousClients xsi:type="ns18:AnonymousCommunication">No</ns18:AnonymousClients>
    <ns18:ServiceAuthorization xsi:type="ns18:ServiceAuthorization">
     <ns18:NoAuthorization xsi:type="ns18:NoAuthorization"/>
    </ns18:ServiceAuthorization>
   </ns18:ServiceSecurity>
