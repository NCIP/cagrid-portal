 <ns3:Services xsi:type="ns3:ServicesType" xmlns:ns3="gme://gov.nih.nci.cagrid.introduce/1/Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns3:Service name="CredentialDelegationService" namespace="http://cds.gaards.cagrid.org/CredentialDelegationService" packageName="org.cagrid.gaards.cds" resourceFrameworkType="main" xsi:type="ns3:ServiceType">
   <ns4:Methods xsi:type="ns4:MethodsType" xmlns:ns4="gme://gov.nih.nci.cagrid.introduce/1/Methods">
    <ns4:Method isImported="true" isProvided="true" name="getServiceSecurityMetadata" xsi:type="ns4:MethodType">
     <ns4:Output qName="ns5:ServiceSecurityMetadata" isArray="false" isClientHandle="false" xmlns:ns5="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"/>
     <ns4:ImportInformation namespace="http://security.introduce.cagrid.nci.nih.gov/ServiceSecurity" packageName="gov.nih.nci.cagrid.introduce.security.stubs" portTypeName="ServiceSecurityPortType" wsdlFile="ServiceSecurity.wsdl"/>
     <ns4:ProviderInformation providerClass="gov.nih.nci.cagrid.introduce.security.service.globus.ServiceSecurityProviderImpl"/>
    </ns4:Method>
    <ns4:Method description="Allows a party to initate the delegation of their credential such that other parties may access their credential to act on their behalf." isImported="false" isProvided="false" name="initiateDelegation" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns6:DelegationRequest" description="" isArray="false" name="req" xmlns:ns6="http://gaards.cagrid.org/cds"/>
     </ns4:Inputs>
     <ns4:Output qName="ns7:DelegationSigningRequest" description="" isArray="false" xmlns:ns7="http://gaards.cagrid.org/cds"/>
     <ns4:Exceptions>
      <ns4:Exception description="" name="CDSInternalFault" qname="ns8:CDSInternalFault" xmlns:ns8="http://cds.gaards.cagrid.org/CredentialDelegationService/types"/>
      <ns4:Exception description="" name="InvalidPolicyFault" qname="ns9:InvalidPolicyFault" xmlns:ns9="http://cds.gaards.cagrid.org/CredentialDelegationService/types"/>
      <ns4:Exception description="" name="DelegationFault" qname="ns10:DelegationFault" xmlns:ns10="http://cds.gaards.cagrid.org/CredentialDelegationService/types"/>
      <ns4:Exception description="" name="PermissionDeniedFault" qname="ns11:PermissionDeniedFault" xmlns:ns11="http://cds.gaards.cagrid.org/CredentialDelegationService/types"/>
     </ns4:Exceptions>
    </ns4:Method>
    <ns4:Method description="Allows the party whom initiated the delegation to approve the delegation." isImported="false" isProvided="false" name="approveDelegation" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns12:DelegationSigningResponse" description="" isArray="false" name="delegationSigningResponse" xmlns:ns12="http://gaards.cagrid.org/cds"/>
     </ns4:Inputs>
     <ns4:Output qName="ns13:DelegatedCredentialReference" clientHandleClass="org.cagrid.gaards.cds.delegated.client.DelegatedCredentialClient" description="" isArray="false" isClientHandle="true" isCreatingResourceForClientHandle="true" resourceClientIntroduceServiceName="DelegatedCredential" xmlns:ns13="http://cds.gaards.cagrid.org/CredentialDelegationService/DelegatedCredential/types"/>
     <ns4:Exceptions>
      <ns4:Exception description="" name="CDSInternalFault" qname="ns14:CDSInternalFault" xmlns:ns14="http://cds.gaards.cagrid.org/CredentialDelegationService/types"/>
      <ns4:Exception description="" name="DelegationFault" qname="ns15:DelegationFault" xmlns:ns15="http://cds.gaards.cagrid.org/CredentialDelegationService/types"/>
      <ns4:Exception description="" name="PermissionDeniedFault" qname="ns16:PermissionDeniedFault" xmlns:ns16="http://cds.gaards.cagrid.org/CredentialDelegationService/types"/>
     </ns4:Exceptions>
    </ns4:Method>
   </ns4:Methods>
