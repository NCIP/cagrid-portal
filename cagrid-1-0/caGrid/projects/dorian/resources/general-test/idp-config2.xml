<idp>
	<resource name="IdPConfiguration" class="gov.nih.nci.cagrid.gums.idp.IdPConfiguration">
		<idp-config>
			<uid-length min="4" max="10"/>
		    <password-length min="6" max="10"/>
		    <registration-policy class="gov.nih.nci.cagrid.gums.idp.ManualRegistrationPolicy"/>
            <asserting-credentials auto="false" renew="false" cert="resources/ca-test/gums-cert.pem" key="resources/ca-test/gums-key.pem"/>
		</idp-config>
	</resource>	
</idp>