<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.cagrid.metrics.common.InvocationEvent"
		table="INVOCATION_EVENTS">
		<id name="id" column="id">
			<generator class="uuid" />
		</id>
		<property name="operationName" column="operation_name"
			type="string" not-null="true" />
		<property name="operationQName" column="operation_qname"
			type="org.cagrid.metrics.service.hibernate.QNameType"/>
		<property name="clientAddress" column="client_address"
			type="string" />
		<property name="clientIdentity" column="client_identity"
			type="string" />
	</class>
</hibernate-mapping>