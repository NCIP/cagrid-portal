<?xml version="1.0" encoding="UTF-8"?>
<grid-portal>
  <resource name="GridPortalConfig" class="org.projectmobius.portal.GridPortalConfig">
    <grid-portal-config>
      <application>
        <name>Introduce: Grid Service Authoring Toolkit</name>
        <icon>/applications-internet.png</icon>
        <initializer>gov.nih.nci.cagrid.introduce.portal.init.IntroducePortalInitializer</initializer>
        <about><![CDATA[<html><center><h2>Introduce: Grid Service Authoring Toolkit</h2></center><center><h3>Version 1.0</h3></center><br><h3>This software is produced by The Ohio State University Department of Biomedicalinformatics (http://bmi.osu.edu)</h3><br><h3>Authors:</h3><br><a href="mailto:hastings@bmi.osu.edu">Shannon Hastings</a><br><a href="mailto:langella@bmi.osu.edu">Stephen Langella</a><br> <a href="mailto:oster@bmi.osu.edu">Scott Oster</a><br><a href="mailto:ervin@bmi.osu.edu">David Ervin</a>]]>
        </about>
        <dimensions>
          <width>1000</width>
          <height>900</height>
        </dimensions>
      </application>
      <components>
        <component>
          <title>Create Service Skeleton</title>
          <description>Create Service Skeleton</description>
          <class>gov.nih.nci.cagrid.introduce.portal.creation.CreationViewer</class>
          <icon>/cog_add.png</icon>
          <dimensions>
            <width>600</width>
            <height>450</height>
          </dimensions>
        </component>
         <component>
          <title>Create caGrid Service Skeleton</title>
          <description>Create caGrid Service Skeleton</description>
          <class>gov.nih.nci.cagrid.introduce.portal.creation.CaBIGCreationViewer</class>
          <icon>/caGrid_small.png</icon>
          <dimensions>
            <width>600</width>
            <height>450</height>
          </dimensions>
        </component>
        <component>
          <title>Modify Service Skeleton</title>
          <description>Modify Service Skeleton</description>
          <class>gov.nih.nci.cagrid.introduce.portal.modification.ModificationViewer</class>
          <icon>/cog_edit.png</icon>
          <dimensions>
            <width>0</width>
            <height>0</height>
          </dimensions>
        </component>
        <component>
          <title>Deploy Grid Service</title>
          <description>Deploy Grid Service</description>
          <class>gov.nih.nci.cagrid.introduce.portal.deployment.DeploymentViewer</class>
          <icon>/cog_go.png</icon>
          <dimensions>
            <width>0</width>
            <height>0</height>
          </dimensions>
        </component>
        <component>
          <title>Browse Data Types</title>
          <description>Browse Data Types</description>
          <class>gov.nih.nci.cagrid.introduce.portal.discoverytools.core.TypesToolsComponent</class>
          <icon>/models.png</icon>
        </component>
      </components>
    </grid-portal-config>
  </resource>
  <resource name="IntroducePortalConf" class="gov.nih.nci.cagrid.introduce.portal.IntroducePortalConf">
    <introduce-portal-config>
    </introduce-portal-config>
  </resource>
</grid-portal>

