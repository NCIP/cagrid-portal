<?xml version="1.0" encoding="UTF-8" ?>
<serviceConfiguration xmlns:ogsi="http://www.gridforum.org/namespaces/2003/03/OGSI" xmlns:aggr="http://www.globus.org/namespaces/2003/09/data_aggregator" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<registrations>
		<registration registry="INDEX-SERVICE-REGISTRY" keepalive="true" lifetime="1200" remove="true">
			<aggr:DataAggregation>
				<ogsi:params>
					<aggr:AggregationSubscription>
						<ogsi:serviceDataNames>
							<ogsi:name xmlns:ce="http://cagrid.nci.nih.gov/1/AnalyticalServiceMetadata">ce:AnalyticalServiceMetadata</ogsi:name>
							<ogsi:name xmlns:ce="http://cagrid.nci.nih.gov/1/CommonServiceMetadata">ce:CommonServiceMetadata</ogsi:name>
						</ogsi:serviceDataNames>
						<aggr:lifetime>60000</aggr:lifetime>
					</aggr:AggregationSubscription>
				</ogsi:params>
			</aggr:DataAggregation>
		</registration>
	</registrations>
</serviceConfiguration>
