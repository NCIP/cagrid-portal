<!-- caGrid Utilities build file                                             -->
<!-- ======================================================================= -->
<project name="caGrid_test" basedir=".">

	<target name="experimental_tests" description="Runs an experimental test build">
		<property file="test/conf/dart.properties" />
		<exec executable="/bin/sh">
			<arg line='-c "test/Dart/Source/Client/Dart"' />
		</exec>
		<exec executable="test\\Dart\\Source\\Client\\Dart">
			<arg value="DartConfiguration.tcl" />
			<arg value="Experimental" />
			<arg value="Start" />
		</exec>
	</target>

</project>