<?xml version="1.0"?>

<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="globus" module="mds" revision="4.0.3">
		<description>The Globus MDS project. Really this should be a number of Globus modules, but
			we currently have no reason to break it out, and its a fairly ellaborate dependency
			tree to mirror.</description>
	</info>

	<configurations>
		<!-- Public Configurations-->
		<conf name="jars" visibility="public"/>
		<conf name="gars" visibility="public"/>
	</configurations>


	<publications>
		<artifact name="globus_wsrf_mds_aggregator" type="jar" conf="jars"/>
		<artifact name="globus_wsrf_mds_aggregator" type="gar" conf="gars"/>
		<artifact name="globus_wsrf_mds_aggregator_stubs" type="jar" conf="jars"/>
		<artifact name="globus_wsrf_aggregator_schema" type="gar" conf="gars"/>

		<artifact name="globus_wsrf_servicegroup" type="jar" conf="jars"/>
		<artifact name="globus_wsrf_servicegroup" type="gar" conf="gars"/>
		<artifact name="globus_wsrf_servicegroup_stubs" type="jar" conf="jars"/>
		<artifact name="globus_wsrf_servicegroup_schema" type="gar" conf="gars"/>

		<artifact name="globus_wsrf_mds_usefulrp" type="gar" conf="gars"/>
		<artifact name="globus_wsrf_mds_usefulrp_schema" type="gar" conf="gars"/>

		<artifact name="globus_wsrf_mds_index" type="gar" conf="gars"/>
		<artifact name="globus_wsrf_mds_index_schema" type="gar" conf="gars"/>

		<artifact name="globus_wsrf_mds_trigger" type="gar" conf="gars"/>
	</publications>

	<dependencies/>
</ivy-module>
