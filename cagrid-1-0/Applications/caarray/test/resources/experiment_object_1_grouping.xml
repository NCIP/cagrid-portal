<?xml version="1.0" encoding="UTF-8"?>
<CQLQuery 
    xmlns="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery
/Users/joshua/dev/ew_cagrid3/cagrid-1-0/caGrid/projects/data/schema/Data/1_gov.nih.nci.cagrid.CQLQuery.xsd">

	<Target name="gov.nih.nci.mageom.domain.Experiment.Experiment">
	    <Group logicRelation="AND">

	        <Group logicRelation="OR">

	            <Association name="gov.nih.nci.mageom.domain.Description.Description" roleName="descriptions">
	                <Association name="gov.nih.nci.mageom.domain.BQS.BibliographicReference" roleName="bibliographicReferences">
	                    <Association name="gov.nih.nci.mageom.domain.Description.DatabaseEntry" roleName="accessions">
	                        <Attribute name="accession" value="15374961 " predicate="EQUAL_TO"/>
	                    </Association>
	                </Association>
	            </Association>
	            <Association name="gov.nih.nci.mageom.domain.AuditAndSecurity.Person" roleName="providers">
	                <Group logicRelation="AND">
	                    <Attribute name="lastName" value="N_lson" predicate="LIKE"/>
	                    <Attribute name="lastName" value="Freije" predicate="EQUAL_TO"/>
	                </Group>
	            </Association>

	        </Group>

	        <Association name="gov.nih.nci.mageom.domain.BioAssay.PhysicalBioAssay" roleName="bioAssays">
	            <Attribute name="identifier" value="gov.nih.nci.ncicb.caarray:PhysicalBioAssay:1015897590498432:1" predicate="EQUAL_TO"/>
	        </Association>
	    </Group>
	</Target>
	
</CQLQuery>
