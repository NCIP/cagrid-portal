<?xml version="1.0" encoding="UTF-8" ?>
<ServiceGroupRegistrationParameters xmlns:sgc="http://mds.globus.org/servicegroup/client"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:wsa="http://schemas.xmlsoap.org/ws/2004/03/addressing"
  xmlns:agg="http://mds.globus.org/aggregator/types"
  xmlns="http://mds.globus.org/servicegroup/client">
  <!-- The ServiceGroupEPR defines the servicegroup to which registrations will be made -->
  <ServiceGroupEPR>
    <wsa:Address>INDEX_SERVICE_URL</wsa:Address>
  </ServiceGroupEPR>
  <!-- Specifies that the registration will be renewed every 10 minutes -->
  <RefreshIntervalSecs>600</RefreshIntervalSecs>
  <Content xsi:type="agg:AggregatorContent" xmlns:agg="http://mds.globus.org/aggregator/types">
    <agg:AggregatorConfig xsi:type="agg:AggregatorConfig">
      <agg:GetMultipleResourcePropertiesPollType
			xmlns:ns21="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BQS"
			xmlns:ns14="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.AuditAndSecurity"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:ns19="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.Measurement"
			xmlns:ns12="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BioSequence"
			xmlns:ns8="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BioAssayData"
			xmlns:ns18="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain"
			xmlns:ns4="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLResultSet"
			xmlns:ns11="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.QuantitationType"
			xmlns:ns20="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.HigherLevelAnalysis"
			xmlns:ns23="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.Protocol"
			xmlns:ns9="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.Experiment"
			xmlns:ns15="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.DesignElement"
			xmlns:ns10="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BioMaterial"
			xmlns:ns3="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery"
			xmlns:ns16="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.Description"
			xmlns:ns2="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"
			xmlns:ns7="http://gov.nih.nci.cagrid.data/DataServiceExceptions"
			xmlns:ns0="http://caarray.cagrid.nci.nih.gov/CaArraySvc/types"
			xmlns:ns6="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.dataservice"
			xmlns:ns24="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.ArrayDesign"
			xmlns:ns1="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"
			xmlns:ns17="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.Array"
			xmlns:ns22="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BioEvent"
			xmlns:ns5="http://caarray.cagrid.nci.nih.gov/CaArraySvc/CQLResultTypes"
			xmlns:ns13="gme://caArray.caBIG/1.1/gov.nih.nci.mageom.domain.BioAssay">
        <!-- Specifies that the index should refresh information
            every 300000 milliseconds (once every 5 minutes) -->
        <agg:PollIntervalMillis>300000</agg:PollIntervalMillis>
        <!-- specifies all Resource Properties that should be retrieved from the service -->
		<agg:ResourcePropertyNames>ns2:ServiceMetadata</agg:ResourcePropertyNames>
	
		<agg:ResourcePropertyNames>ns6:DomainModel</agg:ResourcePropertyNames>
	
      </agg:GetMultipleResourcePropertiesPollType>
    </agg:AggregatorConfig>
    <agg:AggregatorData/>
  </Content>
</ServiceGroupRegistrationParameters>
