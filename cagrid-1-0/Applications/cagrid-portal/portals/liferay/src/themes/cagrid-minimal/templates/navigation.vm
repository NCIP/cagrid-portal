<div style="background-color:#5C5C5C; border-top:1px; border-top-style:solid; border-top-color:#000000; width:100%">
	<table width="363" height="30px" border="0" cellpadding="0" cellspacing="0">
		<tr>
			#foreach ($nav_item in $nav_items)
				#if ($nav_item.isSelected())
					<td width="85" align="center" valign="middle" class="selected_tab">
						<a style="text-decoration:none; text-transform:uppercase;" 
						   href="$nav_item.getURL()" $nav_item.getTarget()>$nav_item.getName()</a>
					</td>
				#else
					<td width="85" align="center" valign="middle" class="normal_tab border_dash">
						<a style="text-decoration:none; text-transform:uppercase; color:#FFFFFF;" 
						   href="$nav_item.getURL()" $nav_item.getTarget()>$nav_item.getName()</a>
					</td>
				#end
				<!--
				#if ($velocityCount lt $nav_items.size())
					<td width="10" align="center" valign="middle" class="normal_tab_pipe" style="color:#FFFFFF;">|</td>
				#end
				-->
			#end
		</td>
	</table>
</div>

