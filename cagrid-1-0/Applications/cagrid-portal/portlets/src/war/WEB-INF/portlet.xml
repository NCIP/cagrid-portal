<?xml version="1.0" encoding="ISO-8859-1"?>

<portlet-app version="1.0"
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
	                             http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">
	                             
	
	<portlet>
		<description>caGrid Registration Portlet</description>
		<portlet-name>cagridregistration</portlet-name>
		<display-name>caGrid Registration Portlet</display-name>
		<portlet-class>
			org.springframework.web.portlet.DispatcherPortlet
		</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/context/register-portlet.xml</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>caGrid Registration Portlet</title>
			<short-title>caGrid Registration Portlet</short-title>
			<keywords>cagrid,registration</keywords>
		</portlet-info>
		
	</portlet>
	
	<portlet>
		<description>caGrid News Summary Portlet</description>
		<portlet-name>cagridnewssummary</portlet-name>
		<display-name>caGrid News Summary Portlet</display-name>
		<portlet-class>
			org.springframework.web.portlet.DispatcherPortlet
		</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/context/newssummary-portlet.xml</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>caGrid News Summary Portlet</title>
			<short-title>caGrid News Summary Portlet</short-title>
			<keywords>cagrid,news</keywords>
		</portlet-info>
		
		<portlet-preferences>
			<preference>
				<name>selectItemForNewsRedirectUrl</name>
				<value>/web/guest/3</value>
			</preference>
			<preference>
                <name>newsItemSelectMode</name> 
                <value>INTERPORTLET</value>
            </preference>
            <preference>
                <name>selectedNewsItemOutputQueueName</name> 
                <value>newssummary:selectedNewsItem</value>
            </preference>
            <preference>
                <name>newssummary:selectedNewsItem</name> 
                <value>news:selectedNewsItem</value>
            </preference>
		</portlet-preferences>
		
	</portlet>
	                 
	<portlet>
		<description>caGrid News Portlet</description>
		<portlet-name>cagridnews</portlet-name>
		<display-name>caGrid News Portlet</display-name>
		<portlet-class>
			org.springframework.web.portlet.DispatcherPortlet
		</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/context/news-portlet.xml</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<portlet-info>
			<title>caGrid News Portlet</title>
			<short-title>caGrid News Portlet</short-title>
			<keywords>cagrid,news</keywords>
		</portlet-info>
		
		<portlet-preferences>
			<preference>
                <name>newsItemSelectMode</name> 
                <value>INTERPORTLET</value>
            </preference>
            <preference>
                <name>selectedNewsItemInputQueueName</name> 
                <value>news:selectedNewsItem</value>
            </preference>
		</portlet-preferences>
		
	</portlet>
	                             
	<portlet>
		<description>caGrid Status Portlet</description>
		<portlet-name>cagridstatus</portlet-name>
		<display-name>caGrid Status Portlet</display-name>
		<portlet-class>
			org.springframework.web.portlet.DispatcherPortlet
		</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/context/status-portlet.xml</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>caGrid Status Portlet</title>
			<short-title>caGrid Status Portlet</short-title>
			<keywords>cagrid,status</keywords>
		</portlet-info>

		<portlet-preferences>
			<preference>
				<name>selectItemForDiscoveryRedirectUrl</name>
				<value>/web/guest/2</value>
			</preference>
			<preference>
				<name>selectDirectoryForDiscoveryRedirectUrl</name>
				<value>/web/guest/2</value>
			</preference>
			<preference>
             	<name>discoveryDirectorySelectMode</name> 
            	<value>INTERPORTLET</value>
         	</preference>
         	<preference>
            	<name>selectedDiscoveryDirectoryOutputQueueName</name> 
            	<value>status:selectedDirectory</value>
         	</preference>
        	<preference>
             	<name>status:selectedDirectory</name>
            	<value>disc:selectedDirectory</value>
        	</preference>
				
			<preference>
             	<name>discoveryItemSelectMode</name> 
            	<value>INTERPORTLET</value>
         	</preference>
         	<preference>
            	<name>selectedDiscoveryItemOutputQueueName</name> 
            	<value>status:selectedItem</value>
         	</preference>
        	<preference>
             	<name>status:selectedItem</name>
            	<value>disc:selectedItem</value>
        	</preference>
        	
		</portlet-preferences>
	</portlet>


	<portlet>
		<description>caGrid Map Portlet</description>
		<portlet-name>cagridmap</portlet-name>
		<display-name>caGrid Map Portlet</display-name>
		<portlet-class>
			org.springframework.web.portlet.DispatcherPortlet
		</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/context/map-portlet.xml</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>caGrid Map Portlet</title>
			<short-title>caGrid Map Portlet</short-title>
			<keywords>cagrid,map</keywords>
		</portlet-info>

		<portlet-preferences>
			<preference>
				<name>selectItemForDiscoveryRedirectUrl</name>
				<value>/web/guest/2</value>
			</preference>	
			<preference>
             	<name>discoveryItemSelectMode</name> 
            	<value>INTERPORTLET</value>
         	</preference>
         	<preference>
            	<name>selectedDiscoveryItemOutputQueueName</name> 
            	<value>map:selectedItem</value>
         	</preference>
        	<preference>
             	<name>map:selectedItem</name>
            	<value>disc:selectedItem</value>
        	</preference>
		</portlet-preferences>
	</portlet>

	<portlet>
		<description>caGrid Discovery Portlet</description>
		<portlet-name>cagriddiscovery</portlet-name>
		<display-name>caGrid Discovery Portlet</display-name>
		<portlet-class>
			org.springframework.web.portlet.DispatcherPortlet
		</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/context/discovery-portlet.xml</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>caGrid Discovery Portlet</title>
			<short-title>caGrid Discovery Portlet</short-title>
			<keywords>cagrid,discovery</keywords>
		</portlet-info>
		
		<portlet-preferences>
		
			<preference>
                <name>discoveryDirectorySelectMode</name> 
                <value>INTERPORTLET</value>
            </preference>
            <preference>
                <name>selectedDiscoveryDirectoryInputQueueName</name> 
                <value>disc:selectedDirectory</value>
            </preference>
		
			<preference>
                <name>discoveryItemSelectMode</name> 
                <value>INTERPORTLET</value>
            </preference>
            <preference>
                <name>selectedDiscoveryItemInputQueueName</name> 
                <value>disc:selectedItem</value>
            </preference>

			<preference>
                <name>umlClassSelectMode</name> 
                <value>INTERPORTLET</value>
            </preference>
            <preference>
                <name>selectedUmlClassOutputQueueName</name> 
                <value>disc:selectedUmlClassId</value>
            </preference>
            <preference>
                <name>disc:selectedUmlClassId</name>
                <value>query:selectedUmlClassId</value>
            </preference>
            
        </portlet-preferences>
		
	</portlet>
	
	<portlet>
		<description>caGrid Query Portlet</description>
		<portlet-name>cagridquery</portlet-name>
		<display-name>caGrid Query Portlet</display-name>
		<portlet-class>
			org.springframework.web.portlet.DispatcherPortlet
		</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/context/query-portlet.xml</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>caGrid Query Portlet</title>
			<short-title>caGrid Query Portlet</short-title>
			<keywords>cagrid,query</keywords>
		</portlet-info>
		
		<portlet-preferences>
			<preference>
                <name>umlClassSelectMode</name> 
                <value>INTERPORTLET</value>
            </preference>
            <preference>
                <name>selectedUmlClassInputQueueName</name> 
                <value>query:selectedUmlClassId</value>
            </preference>
        </portlet-preferences>
	</portlet>

	<portlet>
		<description>Content introducing the caGrid Portal</description>
		<portlet-name>WelcomeToCaGridPortal</portlet-name>
		<display-name>Welcome to caGrid Portal</display-name>
		<portlet-class>
			org.springframework.web.portlet.DispatcherPortlet
		</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/context/content-portlet.xml</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<portlet-info>
			<title>Welcome to caGrid Portal</title>
			<short-title>Welcome</short-title>
			<keywords>welcome</keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>jsp</name>
				<value>welcomeToCaGridPortal</value>
			</preference>
		</portlet-preferences>
	</portlet>
