			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>caGrid CQL Query Results Portlet</title>
			<short-title>caGrid CQL Query Results Portlet</short-title>
			<keywords>cql,query,results</keywords>
		</portlet-info>
		
	</portlet>

