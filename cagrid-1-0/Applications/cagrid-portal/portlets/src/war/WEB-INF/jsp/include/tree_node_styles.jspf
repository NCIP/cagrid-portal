.exp_node{
	list-style: circle url('<c:url value="/images/expandbtn2.gif"/>') inside;
	cursor: pointer;
	cursor: hand;	
}
.coll_node{
	list-style: circle url('<c:url value="/images/collapsebtn2.gif"/>') inside;
	cursor: pointer;
	cursor: hand;	
}
.leaf_node{
	list-style-image: none;
	list-style-type: circle;
	list-style-position: inside;
}
.node_content{
	margin-left: 16px;
}