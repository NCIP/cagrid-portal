        <managed-bean-class>gov.nih.nci.cagrid.browser.beans.SampleQuery</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Experiment Query</value>
        </managed-property>
        <managed-property>
            <property-name>query</property-name>
            <value>
                <![CDATA[
                
<CQLQuery 
    xmlns="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery">
	<Target name="gov.nih.nci.mageom.domain.Experiment.Experiment">
	    <Attribute name="identifier" value="gov.nih.nci.ncicb.caarray:Experiment:1015897558050098:1" predicate="EQUAL_TO"/>
	</Target>
</CQLQuery>
                
                ]]>
            </value>
        </managed-property>
        
    </managed-bean>
    
    
    

