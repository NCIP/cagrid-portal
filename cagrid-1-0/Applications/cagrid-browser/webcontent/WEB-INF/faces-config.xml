            <map-entry>
                <key>http://cagrid-portal.nci.nih.gov:8080/wsrf/services/cagrid/CaBIOSvc</key>
                <value>#{CaBIOSvcSampleQueries}</value>
            </map-entry>
            <map-entry>
                <key>http://cagrid-portal.nci.nih.gov:8080/wsrf/services/cagrid/CaArraySvc</key>
