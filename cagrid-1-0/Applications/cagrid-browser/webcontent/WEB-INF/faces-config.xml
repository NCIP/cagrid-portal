    <managed-bean>
        <managed-bean-name>CaBIOSvcSampleQueries</managed-bean-name>
        <managed-bean-class>java.util.ArrayList</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <list-entries>
            <value-class>gov.nih.nci.cagrid.browser.beans.SampleQuery</value-class>
            <value>#{CaBIOSvcSampleQueries1}</value>
        </list-entries>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>CaBIOSvcSampleQueries1</managed-bean-name>
        <managed-bean-class>gov.nih.nci.cagrid.browser.beans.SampleQuery</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Gene Query</value>
        </managed-property>
        <managed-property>
            <property-name>query</property-name>
            <value>
                <![CDATA[
                
<CQLQuery 
    xmlns="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery">
	<Target name="gov.nih.nci.cabio.domain.Gene">
		<Attribute name="fullName" value="BRCA%" predicate="LIKE"/>
	</Target>
</CQLQuery>
                
                ]]>
            </value>
        </managed-property>
        
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>CaArraySvcSampleQueries</managed-bean-name>
        <managed-bean-class>java.util.ArrayList</managed-bean-class>
