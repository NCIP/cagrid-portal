<?xml version="1.0"?>

<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd">

  <info organisation="caGrid" module="sdkInstaller" revision="${project.version}">
    <license name="caGrid" url="http://cagrid.org/wiki/CaGrid:LICENSE-1.3"/>
    <description>The testings CQL Query Processor</description>
  </info>

  <configurations>
    <conf name="impl-test" visibility="private"/>

    <!-- Public Configurations-->
    <conf name="stubs" visibility="public"/>
    <conf name="default" visibility="public" extends="stubs"/>
    <conf name="test" visibility="public" extends="default,impl-test"/>
  </configurations>


  <publications>
    <artifact name="caGrid-tests-${ivy.module}-stubs" conf="stubs" type="jar"/>
    <artifact name="caGrid-tests-${ivy.module}" conf="default" type="jar"/>
    <artifact name="caGrid-tests-${ivy.module}-tests" conf="test" type="jar"/>
  </publications>

  <dependencies>
    <dependency rev="latest.integration" org="caGrid" name="core"
      conf="default->default;impl-test->test"/>

    <dependency rev="latest.integration" org="caGrid" name="cql" conf="default->cql"/>
    <dependency rev="latest.integration" org="caGrid" name="introduce" conf="default->core"/>

    <dependency rev="3.2.1" org="cacore" name="sdk" conf="default->default"/>
  </dependencies>
</ivy-module>
