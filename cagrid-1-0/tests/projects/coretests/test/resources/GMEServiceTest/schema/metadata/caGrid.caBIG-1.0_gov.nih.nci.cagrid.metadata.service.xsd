      <xs:element name="contextPropertyCollection">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="serv:ContextProperty" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ContextProperty" type="serv:ContextProperty" />
