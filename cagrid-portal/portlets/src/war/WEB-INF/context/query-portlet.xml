            id="selectMySharedQueryToViewController"
            class="gov.nih.nci.cagrid.portal.portlet.query.shared.SelectSharedQueryController"
            autowire="byName">
        <property name="commandName" value="selectSharedQueryCommand"/>
        <property name="commandClass" value="gov.nih.nci.cagrid.portal.portlet.query.shared.SelectSharedQueryCommand"/>
        <property name="actionResponseHandler">
            <bean class="gov.nih.nci.cagrid.portal.portlet.tab.TabActionResponseHandler">
                <property name="tabControlConfig" ref="queryTabControlConfig"/>
                <property name="newTabPath" value="/shared/myQueries/view"/>
