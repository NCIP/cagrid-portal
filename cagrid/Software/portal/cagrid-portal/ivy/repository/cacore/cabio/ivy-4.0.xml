<?xml version="1.0"?>

<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="cacore" module="cabio" revision="4.0">
		<description> The NCICB cabio API provides programmatic interfaces to
             cabio repository.</description>
	</info>

	<configurations>
		<!-- Public Configurations-->
		<conf name="client" visibility="public"/>
	</configurations>


	<publications>
		<artifact name="cabio40-beans" type="jar" conf="client"/>
        <artifact name="cabio40-client-framework" type="jar" conf="client"/>
	</publications>

	<dependencies>
		<dependency rev="4.0" org="cacore" name="api"/>
	</dependencies>
</ivy-module>
